<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="dimYbfUrlCfg">
    <resultMap id="DimYbfUrlCfgMap" type="DimYbfUrlCfg">
        <id property="tblId" column="tbl_id"/>
        <result property="urlId" column="url_id"/>
        <result property="urlName" column="url_name"/>
        <result property="domainName" column="domain_name"/>
        <result property="createdDate" column="created_date"/>
        <result property="createdUser" column="createted_user"/>
        <result property="updatedDate" column="updated_date"/>
        <result property="updatedUser" column="updated_user"/>
    </resultMap>

    <sql id="Base_Column_List">
        tbl_id,
        url_id,
        url_name,
        domain_name,
        created_date,
        created_user,
        updated_date,
        updated_user
    </sql>

    <insert id="insert" parameterType="DimYbfUrlCfg">
        insert into tbl_dm_dim_ybf_url_stat_cfg(
        tbl_id,
        url_id,
        url_name,
        domain_name,
        created_date,
        created_user,
        updated_date,
        updated_user
        )
        values(
        #{tblId},
        #{urlId},
        #{urlName},
        #{domainName},
        #{createdDate},
        #{createdUser},
        #{updatedDate},
        #{updatedUser}
        )
    </insert>

    <select id="getById" resultMap="DimYbfUrlCfgMap" parameterType="string">
        select
        <include refid="Base_Column_List"></include>
        from tbl_dm_dim_ybf_url_stat_cfg
        where tbl_id = #{tblId}
    </select>

    <select id="getByUrlId" resultMap="DimYbfUrlCfgMap" parameterType="string">
        select
        <include refid="Base_Column_List"></include>
        from tbl_dm_dim_ybf_url_stat_cfg
        where url_id = #{urlId}
    </select>

    <select id="search" resultMap="DimYbfUrlCfgMap" parameterType="map">
        select
        <include refid="Base_Column_List"></include>
        from tbl_dm_dim_ybf_url_stat_cfg
        where 1=1
        <if test="urlName != null and urlName != ''">and url_name like CONCAT(CONCAT('%',#{urlName}),'%')</if>
        <if test="urlId != null and urlId != ''">and url_id like CONCAT(CONCAT('%',#{urlId}),'%')</if>
        <if test="domainName != null and domainName != ''">and domain_name like CONCAT(CONCAT('%',#{domainName}),'%')
        </if>
    </select>

    <update id="update" parameterType="DimYbfUrlCfg">
        update
        tbl_dm_dim_ybf_url_stat_cfg
        <set>
            <if test="urlId != null and urlId != ''">url_id = #{urlId}</if>
            <if test="urlName != null and urlName != ''">,url_name = #{urlName}</if>
            <if test="domainName != null and domainName != ''">,domain_name = #{domainName}</if>
            <if test="updatedDate != null">,updated_date = #{updatedDate}</if>
            <if test="updatedUser != null and updatedUser != ''">,updated_user = #{updatedUser}</if>
        </set>
        where tbl_id = #{tblId}
    </update>

    <delete id="deleteById" parameterType="string">
        delete from tbl_dm_dim_ybf_url_stat_cfg
        where tbl_id = #{tblId}
    </delete>

    <select id="getDomains" resultType="string">
        select domain_name from tbl_dm_dim_ybf_url_stat_cfg group by domain_name
    </select>

    <select id="getUrlCfgByDomain" resultMap="DimYbfUrlCfgMap" parameterType="string">
        select
        url_id,url_name
        from tbl_dm_dim_ybf_url_stat_cfg
        where domain_name=#{domainName}
    </select>
</mapper>