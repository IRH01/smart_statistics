<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="infoTypeStatH">


    <select id="findInfoTypeStatH" resultType="infoTypeStatH" parameterType="map">
        select * from tbl_dm_add_info_type_stat_h where 1= 1
        <if test="domain != null">
            and domain_id = #{domain}
        </if>
        <if test="date != null">
            and date_format(etl_date,'%Y-%m-%d') = #{date}
        </if>
        <if test="infoType != null and infoType != ''">
            and info_type_id = #{infoType}
        </if>
    </select>

</mapper>