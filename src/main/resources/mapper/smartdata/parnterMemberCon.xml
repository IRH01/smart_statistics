<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="partnerMemberCon">
    <resultMap type="PartnerMemberCon" id="PartnerMemberConMap">
        <result property="memberId" column="member_id"/>
        <result property="partnerId" column="partner_id"/>
        <result property="partnerNo" column="partner_no"/>
        <result property="partnerName" column="partner_name"/>
    </resultMap>

    <select id="findById" resultMap="PartnerMemberConMap" parameterType="string">
        select
        member_id,
        partner_id,
        partner_no,
        partner_name
        from tbl_dim_partner_member_con
        where member_id = #{memberId}
    </select>
</mapper>