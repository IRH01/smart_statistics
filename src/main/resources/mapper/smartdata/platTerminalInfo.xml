<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="platTerminalInfo">
	<resultMap type="PlatTerminalInfo" id="PlatTerminalInfoMap">
		<result property="id" column="id"/>
		<result property="name" column="name"/>
	</resultMap>
	
	<select id="getAll" resultMap="PlatTerminalInfoMap">
		select 
			id,
			name
		from tbl_gp_plat_terminal_info
	</select>
</mapper>